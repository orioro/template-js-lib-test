{
  "name": "@orioro/template-js-lib-test",
  "version": "0.0.0-semantically-versioned",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "test": "jest",
    "dev": "jest --watch",
    "build": "rm -rf dist && rollup --config ./rollup/build.config.js && npm run tsc-declaration",
    "prepare": "npm run build && npm run readme",
    "readme": "readme",
    "update-dependencies": "npx npm-check-updates -u",
    "tsc-declaration": "tsc --declaration --emitDeclarationOnly --declarationDir dist",
    "commit": "cz"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@orioro/readme": "^0.3.2",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^26.6.3",
    "rollup": "^2.38.4",
    "semantic-release": "^17.3.7",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
